@prefix : <https://cogito.iot.linkeddata.es/tests/process#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc:    <http://purl.org/dc/terms/> .
@prefix dcterms: <http://purl.org/dc/terms/> .

:TestsuiteProcess rdf:type <http://w3id.org/def/vtc#TestSuite>, owl:NamedIndividual;
            dcterms:source "The COGITO Construction Process ontology".

:Test1 rdf:type <https://w3id.org/def/vtc#TestCaseDesign> ,
                owl:NamedIndividual ;
        <https://w3id.org/def/vtc#belongsTo> :TestsuiteProcess;
        dc:description "A task can have a date of creation";
       <https://w3id.org/def/vtc#desiredBehaviour> "Task subclassOf hasCreationDate only datetime" .

:Test2 rdf:type <https://w3id.org/def/vtc#TestCaseDesign> ,
                owl:NamedIndividual ;
        <https://w3id.org/def/vtc#belongsTo> :TestsuiteProcess;
       dc:description "A task can have a conection with other task";
       <https://w3id.org/def/vtc#desiredBehaviour> "Task isExecutedAfter Task" .

:Test3 rdf:type <https://w3id.org/def/vtc#TestCaseDesign> ,
                owl:NamedIndividual ;
        <https://w3id.org/def/vtc#belongsTo> :TestsuiteProcess;
       dc:description "A task includes progress information";
       <https://w3id.org/def/vtc#desiredBehaviour> "Task subclassOf hasProgress only string" .

:Test4 rdf:type <https://w3id.org/def/vtc#TestCaseDesign> ,
                owl:NamedIndividual ;
        <https://w3id.org/def/vtc#belongsTo> :TestsuiteProcess;
        dc:description "A task involves a work order";       
       <https://w3id.org/def/vtc#desiredBehaviour> "Task involvesWork Work" .

:Test5 rdf:type <https://w3id.org/def/vtc#TestCaseDesign> ,
                owl:NamedIndividual ;
        <https://w3id.org/def/vtc#belongsTo> :TestsuiteProcess;
        dc:description "A task can involve workers";              
       <https://w3id.org/def/vtc#desiredBehaviour> "Task involvesResource Worker" .

:Test6 rdf:type <https://w3id.org/def/vtc#TestCaseDesign> ,
                owl:NamedIndividual ;
        <https://w3id.org/def/vtc#belongsTo> :TestsuiteProcess;
       dc:description "A project has a location";         
       <https://w3id.org/def/vtc#desiredBehaviour> "Project isLocatedIn SpatialThing" .

:Test7 rdf:type <https://w3id.org/def/vtc#TestCaseDesign> ,
                owl:NamedIndividual ;
        <https://w3id.org/def/vtc#belongsTo> :TestsuiteProcess;
       dc:description "A task can request a rework on new activities" ;
       <https://w3id.org/def/vtc#desiredBehaviour> "Task involvesWork Work" .

:Test8 rdf:type <https://w3id.org/def/vtc#TestCaseDesign> ,
                owl:NamedIndividual ;
        <https://w3id.org/def/vtc#belongsTo> :TestsuiteProcess;
       dc:description "A task is related to a building elements" ;
       <https://w3id.org/def/vtc#desiredBehaviour> "Task relatesToBuilding Element" .

:Test9 rdf:type <https://w3id.org/def/vtc#TestCaseDesign> ,
                owl:NamedIndividual ;
        <https://w3id.org/def/vtc#belongsTo> :TestsuiteProcess;
       dc:description "A task has a  KPI" ;
       <https://w3id.org/def/vtc#desiredBehaviour> "Task hasKPI KeyPerformanceIndicator" .
